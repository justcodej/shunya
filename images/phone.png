*{
    padding: 0;
    margin: 0;
}
body{
    font-family: "Microsoft Yahei";
    font-size: 14px;
}
li{
    list-style: none;
}





/*导航开始*/
#nav{
    position: fixed;
    top: 0;
    width: 100vw;
}
#nav .top_bar{
    height: 25px;
    border-bottom: 1px solid #e4e4e4;
    overflow: hidden;
    background-color: #f9f9f9;
}
#nav .top_bar .content{
    position: relative;
    width: 1132px;
    height: 100%;
    margin: 0 auto;
}
#nav .top_bar ul{
    position: absolute;
    right: 0;
    top: 0;
    display: flex;
    overflow: hidden;
}